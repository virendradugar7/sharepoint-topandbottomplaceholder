{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "custom-header-footer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/customHeaderFooter/CustomHeaderFooterApplicationCustomizer.js",
          "manifest": "./src/extensions/customHeaderFooter/CustomHeaderFooterApplicationCustomizer.manifest.json"
        }
      ]
    },
    "footer-customizer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/footerCustomizer/FooterCustomizerApplicationCustomizer.js",
          "manifest": "./src/extensions/footerCustomizer/FooterCustomizerApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CustomHeaderFooterApplicationCustomizerStrings": "lib/extensions/customHeaderFooter/loc/{locale}.js",
    "FooterCustomizerApplicationCustomizerStrings": "lib/extensions/footerCustomizer/loc/{locale}.js"
  }
}
